{
    "openapi": "3.0.0",
    "info": {
        "title": "IBM COS Pre-Signed URL Generator",
        "description": "Generate pre-signed URLs for accessing objects in IBM Cloud Object Storage.",
        "version": "1.0.0"
    },
    "servers": [
        {
          "url": "Add codeengine/dockerhub url where you deployed your docker image Ex : https://pregenurl-v1.test1234.us-south.codeengine.appdomain.cloud/" 
        }
    ],
    "paths": {
        "/generate-url/": {
            "get": {
                "summary": "Generate Presigned Url",
                "operationId": "generate_presigned_url_generate_url__get",
                "parameters": [
                    {
                        "name": "cos_access_key_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Cos Access Key Id"
                        }
                    },
                    {
                        "name": "cos_secret_access_key",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Cos Secret Access Key"
                        }
                    },
                    {
                        "name": "cos_endpoint_url",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Cos Endpoint Url"
                        }
                    },
                    {
                        "name": "expiration",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "title": "Expiration"
                        }
                    },
                    {
                        "name": "key_name",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Key Name"
                        }
                    },
                    {
                        "name": "bucket_name",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Bucket Name"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "presignedurl": {
                                            "type": "string",
                                            "description": "Description of the response variable"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "type": "object",
                "properties": {
                    "detail": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            },
            "ValidationError": {
                "type": "object",
                "properties": {
                    "loc": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "msg": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    }
                },
                "required": ["loc", "msg", "type"]
            }
        }
    }
}
